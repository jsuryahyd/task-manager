{
	"name": "Task List",
	"version": "0.0.1",
	"description": "Capture and Replay series of interactions",
	"manifest_version": 3,
	"permissions": [
		"webRequest",
		"declarativeContent",
		"activeTab",
		"webNavigation",
		"storage"
	],
	"background": {
		"type": "module",
		"service_worker": "./background/background.js"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'"
	},
	"action": {},
	"icons": {
		"128": "list.png"
	}
}